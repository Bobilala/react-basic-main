{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/*",
      "methods": [
        "get",
        "post"
      ],
      "allowedRoles": [
        "test"
      ]
    },
    {
      "route": "/",
      "methods": [
        "get",
        "post"
      ],
      "redirect": "/.auth/login/aad",
      "statusCode": 401,
      "allowedRoles": [
        "administrator"
      ]
    }
  ]
}